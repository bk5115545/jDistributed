import java.io.File;


public class Program {

	File toExecute = null;
	Class toCall = null;
	
	public Program(Class c, File toE) {
		toCall = c;
		toExecute = toE;
	}
	
	public void begin() {
		try {
			Object instance = toCall.newInstance();
		} catch (InstantiationException | IllegalAccessException e) {
			e.printStackTrace();
		}
	}
	
}
