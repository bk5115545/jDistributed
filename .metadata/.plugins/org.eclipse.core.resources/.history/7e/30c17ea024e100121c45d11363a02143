import java.io.File;
import java.io.FileNotFoundException;


public class ConfigLoader {

	private static String filename = "Config.conf";
	
	
	public ConfigLoader() {
		File f = new File(filename);
		if(f==null || !f.exists()) {
			if(f== null) {
				throw new NullPointerException("f is null for filename: " + filename); 
				return;
			}
			if(f.canWrite()) {
				
			}
		}
	}
}
